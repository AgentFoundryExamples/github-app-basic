# Example environment configuration file
# Copy this to .env and fill in your values

# Application Environment (dev/prod)
APP_ENV=dev

# Server Configuration
PORT=8000
LOG_LEVEL=INFO

# GCP Configuration (optional for dev, required for prod deployments)
# GCP_PROJECT_ID=your-project-id
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json
REGION=us-central

# GitHub App Configuration (required for production)
# GITHUB_APP_ID=123456
# GITHUB_PRIVATE_KEY=-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----
# GITHUB_CLIENT_ID=Iv1.xxxxx
# GITHUB_CLIENT_SECRET=xxxxxx
# GITHUB_WEBHOOK_SECRET=xxxxxx

# CORS Configuration (disabled by default)
ENABLE_CORS=false
